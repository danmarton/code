delegate bool Compare(Emp x, Emp y);

class MainClass
{
    static void Main()
    {
        
    }
}

class Emp
{
    string Name;
    int Hiredate;
    
    public Emp(n,h)
    {
        this.Name = n;
        this.Hiredate = h;
    }
    
    static bool Compare(Emp x, Emp y)
    {
        if (x.Hiredate < y.Hiredate) return true;
    }
}
